package com.sutherland.loan.repo;

import org.springframework.cloud.openfeign.FeignClient;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.PathVariable;

import com.sutherland.loan.dto.CreditScore;
@FeignClient(name ="creditscore-service")
public interface CreditScoreServiceProxy {
	@GetMapping(value ="/creditscore/bypancard/{pancard}")
	CreditScore (@PathVariable String pancard);

	CreditScore getCreditScore(String pancard);
	

}
